<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <StartAction>Program</StartAction>
    <StartProgram>$([System.Environment]::GetEnvironmentVariable(`WinDir`))\explorer.exe</StartProgram>
    <StartArguments>$(MSBuildThisFileDirectory)bin\html</StartArguments>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="WebSharper" Version="4.6.0.356" />    
    <PackageReference Include="WebSharper.CSharp" Version="4.6.0.356" />
    <PackageReference Include="WebSharper.UI" Version="4.6.0.184" />
    <PackageReference Include="WebSharper.UI.CSharp" Version="4.6.0.184" />
    <None Include="wsconfig.json" />
  </ItemGroup>

</Project>