<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" xmlns:sdk="http://schemas.microsoft.com/developer/vstemplate-sdkextension/2010">
  <TemplateData>
    <Name>WebSharper 4 Suave-hosted Site</Name>
    <Description>Creates a starter client-server application based on sitelets and UI.Next running on Suave.</Description>
    <Icon>WebSharper.png</Icon>
    <ProjectType>FSharp</ProjectType>
    <RequiredFrameworkVersion>4.6</RequiredFrameworkVersion>
    <SortOrder>1000</SortOrder>
    <TemplateID>82fe23b2-4348-4b36-8447-74255d804b46</TemplateID>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Suave</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
  </TemplateData>
  <TemplateContent>
    <Project File="ProjectTemplate.fsproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true">AssemblyInfo.fs</ProjectItem>
      <ProjectItem ReplaceParameters="true" OpenInEditor="true">Main.fs</ProjectItem>
      <ProjectItem ReplaceParameters="true">Client.fs</ProjectItem>
      <ProjectItem ReplaceParameters="true">Remoting.fs</ProjectItem>
      <ProjectItem ReplaceParameters="true">Main.html</ProjectItem>
    </Project>
  </TemplateContent>
  <WizardExtension>
    <Assembly>Zafir.4.0.187.0-beta10, Version=1.0.0.0, Culture=neutral, PublicKeyToken=dcd983dec8f76a71</Assembly>
    <FullClassName>WebSharper.Vsix.VSVersionWizard</FullClassName>
  </WizardExtension>
  <WizardExtension>
    <Assembly>NuGet.VisualStudio.Interop, Version=1.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</Assembly>
    <FullClassName>NuGet.VisualStudio.TemplateWizard</FullClassName>
  </WizardExtension>
  <WizardData>
    <packages repository="extension" repositoryId="IntelliFactory.WebSharper.371cf828-9e17-41cb-b014-496f3e9e7171">
      <package id="System.ValueTuple" version="4.3.0" targetFramework="net40" />
      <package id="Zafir" version="4.0.187.90-beta10" targetFramework="net46" />
      <package id="Zafir.FSharp" version="4.0.187.90-beta10" targetFramework="net46" />
      <package id="Zafir.UI.Next" version="4.0.167.77-beta10" targetFramework="net46" />
      <package id="Suave" version="2.2.1" targetFramework="net46" />
      <package id="Owin" version="1.0.0" targetFramework="net46" />
      <package id="Zafir.Owin" version="4.0.91.48-beta10" targetFramework="net46" />
      <package id="Microsoft.Owin" version="3.1.0" targetFramework="net46" />
      <package id="Zafir.Suave" version="4.0.103.56-beta10" targetFramework="net46" />
    </packages>
